---
permalink: "_direct-access/{{metadata.courseCode}}_DA.html"
---

<!DOCTYPE html>
<html lang="en">
  <head
		data-module-code="{{metadata.courseCode}}"
		data-access-type="{{metadata.dataAccessType}}"
		data-course-mode="{{metadata.dataCourseMode}}"
		data-course-provider="{{metadata.dataCourseProvider}}"
	>
    <meta charset="UTF-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>{{ metadata.courseTitle }}</title>
    {% set css %}
    {% include "../_shared/css/guide.css" %}
    {% include "../_shared/css/shared.css" %}
    {% endset %}

    {% if devEnvironment.environment === 'dev' %}
      <link rel="stylesheet" href="/_shared/css/guide.css">
      <link rel="stylesheet" href="/_shared/css/shared.css">
    {% else %}
      <style>
        {{css | cssmin | safe}}
      </style>
    {% endif %}
    <link rel="stylesheet" href="styles.css"/>
  </head>
  <body>

    <div class="ians-lms-banner not-sr-only">
      <a
				target="content-iframe"
				href="{{metadata.courseCode}}_syllabus.html"
				id="backToSyllabus"
				name="link-syllabus"
				title="Back to syllabus"
			>
				&lt;&lt; Back to Syllabus
			</a>
    </div>

    <iframe
     sandbox="allow-same-origin allow-scripts"
			id="contentIframe"
			name="content-iframe"
			src="{{metadata.courseCode}}_syllabus.html"
			frameborder="0"
			width="100%"
			height="100%"
			seamless
			allowfullscreen
		></iframe>

    <script src="main.js"></script>
  </body>
</html>